<view bind:__l="__l"><view class="user_head"><view class="user_avatar"><image src="{{userInfo.avatarUrl}}" mode="aspectFit"></image><text>{{userInfo.nickName}}</text></view><view class="user_order"><view class="user_my_order">我的订单</view><view class="uesr_menu_order"><block wx:for="{{userMenu}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uesr_item_order"><view class="user_item_img"><image style="{{'width:'+(item.width+'px')+';'+('height:'+(item.height+'px')+';')}}" src="{{item.url}}" mode="aspectFit"></image></view><text>{{item.name}}</text></view></block></view><block wx:for="{{userCarte}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="user_menu_con" id="{{item.id}}" data-event-opts="{{[['tap',[['clickCarte',['$event']]]]]}}" bindtap="__e"><view class="user_menu_left"><image style="{{'width:'+(item.width+'px')+';'+('height:'+(item.height+'px')+';')}}" src="{{item.url}}" mode="aspectFit"></image><text>{{item.name}}</text></view><view class="user_menu_right"></view></view></block></view></view></view>